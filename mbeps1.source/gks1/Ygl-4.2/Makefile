# ----------------------------------------------------------------------
# start of Imakefile

#
#    Ygl: Run GL programs with standard X11 routines.
#    (C) Fred Hucht 1993-2007
#    EMail: fred(AT)thp.Uni-Duisburg.de
#
#    $Id: Imakefile,v 4.5 2007-05-08 11:00:41+02 fred Exp $

#  Set UseX11 to 1 if you want X11 bindings (faster, but only 2d)

#  Set UseOpenGL to 1 if you want OpenGL bindings
#  (might be slower than X11, but also 3d)

#  Set Has_GLX_SGI_video_sync to 0 if you get link errors like
#  "ld: 0711-317 ERROR: Undefined symbol: .glXGetVideoSyncSGI"
#  (e.g. under AIX 4.3)

#  Set DoubleBuffer to 1 if your system has the include file
#  <X11/extensions/Xdbe.h> and the library libXext.a
#  This should be true for X11R6.1. Only nessesary with 1 == 1

#  Set MultiBuffer to 1 if your system has the include file
#  <X11/extensions/multibuf.h> and the library libXext.a
#  This should be true for X11R5. Only nessesary with 1 == 1

#  Set FortranBindings to 1 to include FORTRAN bindings into
#  the Ygl library.

#  Set UseGlobalColormap to 1 to use experimental colormap handling.
#  You must have the include file <X11/Xmu/StdCmap.h> and libXmu.a

#  Set YglPrefix to 1 to prepend "ygl_" to all function names

#  ******* The following stuff is obsolete! ***************
#  Set UseMesa to 1 if you want OpenGL bindings using Mesa.
#  Don\'t define both 1 and UseMesa!
#  If you installed Mesa with the OpenGL names (libGL.a etc.)
#  set 1 to 1.
#  You may have to change the path to the Mesa lib below...

# ***************** End of configuable part ********************

SOYGLREV = 4.2

OGL		=	-DOGL
OGLIB		=	-lGL -lGLU

X11		=	-DX11

GSvs		=	-DGSvs

DOUBLEBUF	=	-DDOUBLEBUF

MULTIBUF	=	-DMULTIBUF

DMBUFLIB 	= 	-lXext

TARGET		= 	Ygl

DEFINES 	= 	$(OGL) $(X11) $(GSvs) $(DOUBLEBUF) $(MULTIBUF) $(HASXMU) $(YGL_PREFIX)

FSRC 		= 	fortran.c
FOBJ 		= 	fortran.o
FHDR 		= 	X11/Yfgl.h

SRCS = ygl.c draw.c misc.c font.c queue.c color.c menu.c gl2ppm.c 3d.c $(FSRC)
OBJS = ygl.o draw.o misc.o font.o queue.o color.o menu.o gl2ppm.o 3d.o $(FOBJ)

INCLUDES 	= 	-I.
XLIBS		= 	$(OGLIB) $(DMBUFLIB) $(THEXMULIB) -lX11
HEADERS 	= 	X11/Ygl.h X11/Ygltypes.h $(FHDR) $(YGL_PREFIX_HDR)
INCDIR		=	$(INCROOT)/X11

REQUIREDLIBS	=	-L$(USRLIBDIR) $(XLIBS)

# This stuff and more contributed by Larry Schwimmer <rosebud@cyclone.Stanford.EDU>

# $Xorg: Library.tmpl,v 1.3 2000/08/17 19:41:46 cpqbld Exp $

# $XFree86: xc/config/cf/Library.tmpl,v 3.23tsi Exp $

         CC = /usr/bin/cc

  CCOPTIONS = -Wall -Wpointer-arith -no-cpp-precomp -fno-common
STD_DEFINES = -D__i386__ -D__DARWIN__                                 -DNO_ALLOCA -DCSRG_BASED  $(PROJECT_DEFINES)
CDEBUGFLAGS = -Os
CLIBDEBUGFLAGS =
     CFLAGS = $(CDEBUGFLAGS) $(CLIBDEBUGFLAGS) $(CCOPTIONS) $(THREADS_CFLAGS) $(ALLDEFINES)

LIBNAME = $(TARGET)

all::
	$(_NULLCMD_)
	$(_NULLCMD_)
	@if [ -d unshared ]; then exit 0; else set -x; mkdir unshared; fi
	$(_NULLCMD_)

includes::
	$(_NULLCMD_)
	$(_NULLCMD_)
	@if [ -d unshared ]; then exit 0; else set -x; mkdir unshared; fi
	$(_NULLCMD_)

.c.o:
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(RM) $@ unshared/$@
	 	$(CC) -c $(CCOPTIONS) $(THREADS_CFLAGS) $(ALLDEFINES) 	$(CDEBUGFLAGS) $(CLIBDEBUGFLAGS) $(_NOOP_) $*.c -o unshared/$@
	$(_NULLCMD_)
	$(RM) $@
	 	$(CC) -c $(CFLAGS) $(_NOOP_) $(SHLIBDEF) $(SHAREDCODEDEF) $(PICFLAGS) $*.c

cleandir::
	$(_NULLCMD_)
	$(_NULLCMD_)
	-@if [ -d unshared ]; then \
		set -x; $(RM) -r unshared; else exit 0; fi
	$(_NULLCMD_)

all:: lib$(LIBNAME).$(SOYGLREV).dylib

lib$(LIBNAME).$(SOYGLREV).dylib:  $(OBJS) $(EXTRASHAREDOBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@~
	@MAJREV=`echo $(SOYGLREV) | sed 's/\([0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`; \
	  INSTALLNAME=lib$(LIBNAME).$$MAJREV.dylib; \
	  set -x; (cd .; $(CC) -o ./$@~ $(SHLIBLDFLAGS) -install_name $(USRLIBDIR)/$$INSTALLNAME -current_version $(SOYGLREV) -compatibility_version $(SOYGLREV) $(OBJS) $(EXTRASHAREDOBJS) $(REQUIREDLIBS)); \
	  $(RM) $$INSTALLNAME; $(LN) $@ $$INSTALLNAME; \
	  true
	$(RM) $@
	$(MV) $@~ $@
	@if $(SOSYMLINK); then (set -x; \
	  $(RM) lib$(LIBNAME).dylib; \
	  $(LN) $@ lib$(LIBNAME).dylib); fi
	$(_NULLCMD_)
	$(_NULLCMD_)

cleandir::
	@MAJREV=`echo $(SOYGLREV) | sed 's/\([0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`; \
	set -x; $(RM) lib$(LIBNAME).$$MAJREV.dylib
	$(RM) lib$(LIBNAME).$(SOYGLREV).dylib lib$(LIBNAME).dylib

install:: lib$(LIBNAME).$(SOYGLREV).dylib
	@if [ -d $(DESTDIR)$(SHLIBDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(SHLIBDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(SHLIBDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(SHLIBDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) lib$(LIBNAME).$(SOYGLREV).dylib $(DESTDIR)$(SHLIBDIR)
	@MAJREV=`echo $(SOYGLREV) | sed 's/\([0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`; \
	  set -x; $(RM) $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).$$MAJREV.dylib && \
	  $(LN) lib$(LIBNAME).$(SOYGLREV).dylib $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).$$MAJREV.dylib
	@if $(SOSYMLINK); then (set -x; \
	  $(RM) $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).dylib; \
	  $(LN) lib$(LIBNAME).$(SOYGLREV).dylib $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).dylib); fi

all:: lib$(LIBNAME).a

lib$(LIBNAME).a: $(OBJS) $(EXTRAUNSHAREDOBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	@(set -x; cd unshared && $(AR) ../$@ $(OBJS) $(EXTRAUNSHAREDOBJS)) || exit 1;
	$(RANLIB) $@
	$(_NULLCMD_)

install:: lib$(LIBNAME).a
	@if [ -d $(DESTDIR)$(USRLIBDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(USRLIBDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(USRLIBDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(USRLIBDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) lib$(LIBNAME).a $(DESTDIR)$(USRLIBDIR)
	$(RANLIB) $(RANLIBINSTFLAGS) $(DESTDIR)$(USRLIBDIR)/lib$(LIBNAME).a

lintlib:: llib-l$(LIBNAME).ln

llib-l$(LIBNAME).ln: $(SRCS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)$(LIBNAME) $(LINTFLAGS) $(SRCS)

install.ln:: llib-l$(LIBNAME).ln
	@if [ -d $(DESTDIR)$(LINTLIBDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(LINTLIBDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(LINTLIBDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(LINTLIBDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) llib-l$(LIBNAME).ln $(DESTDIR)$(LINTLIBDIR)/llib-l$(LIBNAME).ln

install:: $(HEADERS)
	@if [ -d $(DESTDIR)$(INCDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(INCDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(INCDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)); \
	fi
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[i]*) set +e;; esac; done; \
	for i in $(HEADERS); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTINCFLAGS) $$i $(DESTDIR)$(INCDIR)); \
	done

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

SOSYMLINK = true

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

$(YGL_PREFIX_HDR):	makeYglprefix X11/Ygl.h
	./makeYglprefix > $@

X11/Ygltypes.h:		makeYgltypes
	./makeYgltypes > $@

